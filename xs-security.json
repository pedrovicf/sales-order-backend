{
    "xsappname": "sales-order-backend",
    "tenant-mode": "dedicated",
    "description": "Security profile of Sales Order Backend App",
    "scopes": [
        {
            "name": "uaa.user",
            "description": "UAA user"
        },
        {
            "name": "$XSAPPNAME.admin",
            "description": "Admin user"
        },
        {
            "name": "$XSAPPNAME.read_only_user",
            "description": "ReadOnly user"
        } 
    ],
    "attributes": [
        {
            "name": "id",
            "description": "User ide",
            "valueType": "string",
            "valueRequired": false
        },
        {
            "name": "groups",
            "description": "User groups",
            "valueType": "string",
            "valueRequired": false
        }
    ],
    "role-templates": [
        {
           "name": "Token_Exchange",
           "description": "UAA",
           "scope-references": [
               "uaa.user"
           ] 
        },
        {
           "name": "Admin", 
           "description": "Admin user",
           "attribute-references": [ 
               "id",
               "groups"
           ],
           "scope-references": [
               "$XSAPPNAME.admin"
           ] 
        },
        {
           "name": "ReadOnlyUser", 
           "description": "ReadOnly user",
           "attribute-references": [ 
               "id",
               "groups"
           ],
           "scope-references": [
               "$XSAPPNAME.read_only_user"
           ] 
        }
    ],
    "oauth2-configuration": {
        "redirect-uris": [
            "https://789a18f8trial-*/**"
        ]
    }
}